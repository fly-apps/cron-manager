{
  "schedules": [
    {
      "name": "dsm-station-refresh",
      "app": "dsm",
      "image":"registry.fly.io/tri-cron-manager:dsm",
      "command": "python -m tri.cli.main dsm run --refresh_stations=True",
      "schedule": "0 0 * * 0"
    },
    {
      "name": "dsm-daily",
      "app": "dsm",
      "image":"registry.fly.io/tri-cron-manager:dsm",
      "command": "python -m tri.cli.main dsm run --daily=True",
      "schedule": "0 6 * * *"
    }
  ]
}
